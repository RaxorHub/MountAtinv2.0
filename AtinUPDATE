-- ==== MAKE TELEPORT BUTTONS (obfuscated function) ====
local function _mk(nm, pos)
    local _tb = Instance.new(_S{84,101,120,116,66,117,116,116,111,110})
    _tb.Size = UDim2.new(0,140,0,35)
    _tb.BackgroundColor3 = Color3.fromRGB(40,40,40)
    _tb.Text = nm
    _tb.TextColor3 = Color3.new(1,1,1)
    _tb.Font = Enum.Font.SourceSansBold
    _tb.TextSize = 14
    _tb.Parent = _scr

    local _cc = Instance.new(_S{85,73,67,111,114,110,101})
    _cc.CornerRadius = UDim.new(0,8)
    _cc.Parent = _tb

    _tb.MouseButton1Click:Connect(function()
        local ch = _p.Character
        if ch and ch:FindFirstChild(_S{72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116}) then
            ch[_S{72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116}].CFrame = CFrame.new(pos)
        end
    end)
end

for _,v in ipairs(_tp) do
    _mk(v.n, v.p)
end

-- ==== AUTO FARM LOGIC (keep original behavior) ====
local _pos1 = Vector3.new(625.27,1799.83,3432.84)
local _pos2 = Vector3.new(780.47,2183.38,3945.07)
local _tflag = false

local function _tele(p)
    local ch = _p.Character
    if ch and ch:FindFirstChild(_S{72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116}) then
        ch[_S{72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116}].CFrame = CFrame.new(p)
    end
end

local function _loop()
    while _tflag do
        _tele(_pos1)
        wait(2)
        _tele(_pos2)
        wait(1)
        _c:Teleport(game.PlaceId, _p)
        break
    end
end

local function _toggle(s)
    _tflag = s
    _status.Value = s
    if _tflag then
        _btn.Text = _S{79,78} -- "ON"
        _btn.BackgroundColor3 = Color3.new(0,1,0)
        spawn(_loop)
    else
        -- original had same values; preserve behavior
        _btn.Text = _S{79,78}
        _btn.BackgroundColor3 = Color3.new(0,1,0)
    end
end

_btn.MouseButton1Click:Connect(function()
    _toggle(not _tflag)
end)

-- ==== REJOIN NOTIFICATION (obfuscated) ====
local function _showRejoin()
    local _ng = Instance.new(_S{83,99,114,101,101,110,71,117,105})
    _ng.Name = _S{82,101,106,111,105,110,78,111,116,105,102} -- "RejoinNotif"
    _ng.Parent = _p:WaitForChild(_S{80,108,97,121,101,114,71,117,105})

    local _nf = Instance.new(_S{70,114,97,109,101})
    _nf.Size = UDim2.new(0,220,0,50)
    _nf.Position = UDim2.new(0.5,-110,0.1,0)
    _nf.BackgroundColor3 = Color3.new(0,0,0)
    _nf.BackgroundTransparency = 0.25
    _nf.BorderSizePixel = 0
    _nf.Parent = _ng

    local _nt = Instance.new(_S{84,101,120,116,76,97,98,101,108})
    _nt.Size = UDim2.new(1,0,1,0)
    _nt.BackgroundTransparency = 1
    _nt.Text = _S{82,101,106,111,105,110,101,100,33,46,46,46} -- "Rejoined!..."
    _nt.TextColor3 = Color3.new(0,1,0)
    _nt.TextScaled = true
    _nt.Font = Enum.Font.SourceSansBold
    _nt.Parent = _nf

    wait(1)
    local _t = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local _tw = _a:Create(_nf, _t, {BackgroundTransparency = 1})
    local _tw2 = _a:Create(_nt, _t, {TextTransparency = 1})

    _tw:Play(); _tw2:Play()
    _tw.Completed:Connect(function() _ng:Destroy() end)
end

-- ==== HANDLE REJOIN / CHARACTER ADDED ====
_p.CharacterAdded:Connect(function()
    wait(2)
    if _status.Value then
        _toggle(true)
        _showRejoin()
    end
end)

if _p.Character and _status.Value then
    _toggle(true)
    _showRejoin()
end
                          
