-- By : Xraxor CMNY 
local function _S(c) return string.char(unpack(c)) end

local _a = game:GetService(_S{84,119,101,101,110,83,101,114,118,105,99,101}) -- TweenService
local _b = game:GetService(_S{80,108,97,121,101,114,115}) -- Players
local _c = game:GetService(_S{84,101,108,101,112,111,114,116,83,101,114,118,105,99,101}) -- TeleportService
local _d = game:GetService(_S{82,101,112,108,105,99,97,116,101,100,83,116,111,114,97,103,101}) -- ReplicatedStorage
local _e = game:GetService(_S{85,115,101,114,73,110,112,117,116,83,101,114,118,105,99,101}) -- UserInputService

local _p = _b.LocalPlayer

-- small randomizer util
local function _r(n) local s="" for i=1,n do s = s .. string.char(math.random(97,122)) end return s end
math.randomseed(tick()%2^31)

-- ==== INTRO ANIMATION (encoded strings) ====
do
    local _gui = Instance.new(_S{83,99,114,101,101,110,71,117,105})
    _gui.Name = _S{73,110,116,114,111,65,110,105,109} -- "IntroAnim"
    _gui.ResetOnSpawn = false
    _gui.Parent = _p:WaitForChild(_S{80,108,97,121,101,114,71,117,105}) -- "PlayerGui"

    local _lbl = Instance.new(_S{84,101,120,116,76,97,98,101,108})
    _lbl.Size = UDim2.new(0,300,0,50)
    _lbl.Position = UDim2.new(0.5,-150,0.4,0)
    _lbl.BackgroundTransparency = 1
    _lbl.Text = _S{66,121,32,58,32,88,114,97,120,111,114} -- "By : Xraxor"
    _lbl.TextColor3 = Color3.fromRGB(40,40,40)
    _lbl.TextScaled = true
    _lbl.Font = Enum.Font.FredokaOne
    _lbl.Parent = _gui

    local _ti1 = TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local _move = _a:Create(_lbl, _ti1, {Position = UDim2.new(0.5, -150, 0.42, 0)})
    local _ti2 = TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local _scale = _a:Create(_lbl, _ti2, {TextSize = 50})
    local _ti3 = TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local _color = _a:Create(_lbl, _ti3, {TextColor3 = Color3.fromRGB(0,180,0)})

    _move:Play(); _scale:Play(); _color:Play()
    wait(2)
    local _f = _a:Create(_lbl, TweenInfo.new(0.5), {TextTransparency = 1})
    _f:Play()
    _f.Completed:Connect(function() _gui:Destroy() end)
end

-- ==== STATUS HOLDER (ReplicatedStorage BoolValue) ====
local function _ensureStatus()
    local _key = _d:FindFirstChild(_S{65,117,116,111,70,97,114,109,83,116,97,116,117,115}) -- "AutoFarmStatus"
    if _key and _key:IsA(_S{66,111,111,108,86,97,108,117,101}) then
        return _key
    end
    local _n = Instance.new(_S{66,111,111,108,86,97,108,117,101})
    _n.Name = _S{65,117,116,111,70,97,114,109,83,116,97,116,117,115}
    _n.Value = false
    _n.Parent = _d
    return _n
end
local _status = _ensureStatus()

